{
    "name": "sebdesign/laravel-sri",
    "description": "Subresource Integrity (SRI) package for Laravel",
    "keywords": [
        "laravel",
        "sri"
    ],
    "homepage": "https://github.com/sebdesign/laravel-sri",
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "laravel/framework": "^5.5|^6.20.26|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.3",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Sebdesign\\SRI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebdesign\\SRI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Nikolaou",
            "email": "info@sebdesign.eu",
            "homepage": "http://sebdesign.eu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Sebdesign\\SRI\\SubresourceIntegrityServiceProvider"
            ]
        }
    }
}
